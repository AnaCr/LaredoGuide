<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.acruz.laredoguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BrowseRestaurantsActivity"
            android:label="@string/restaurants" />
        <activity
            android:name=".BrowseEntertainmentActivity"
            android:label="@string/entertainment" />
        <activity
            android:name=".BrowseRecreationalActivity"
            android:label="@string/recreational" />
        <activity
            android:name=".BrowseEducationalActivity"
            android:label="@string/educational" />
        <activity
            android:name=".DetailsActivity"
            android:label="@string/details"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>